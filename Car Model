public class Car {
	//Car variables
	int speed;
	int positionX;
	int positionY;
	
	//constructor class
	public Car(int positionX, int positionY){
		this.speed = 0;
		this.positionX = positionX;
		this.positionY = positionY;
	}
	
	//get coordinates
	public String getCar(){
		String answer = "positionX: " + positionX + " " + "positionY: " + positionY;
		return answer;
	}
	
	//move Car
	public void moveCarRight(){
		this.positionX += 1;
	}
	
	public void moveCarDown(){
		this.positionY += 1;
	}
}
